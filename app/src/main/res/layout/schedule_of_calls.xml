                android:textColor="@color/TextButton"
                android:id="@+id/Back_BTN"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="@string/Back"
                android:onClick="Back"
                android:textAllCaps="false"
                android:fontFamily="@font/golos_text_regular"
                android:letterSpacing="0"
                android:paddingHorizontal="6dp"
                app:icon="@drawable/ic_back"
                app:iconTint="@color/BrandColor"
                style="@style/Widget.MaterialComponents.Button.TextButton"/>

        </LinearLayout>

        <LinearLayout
            android:gravity="center"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1">

            <TextView
                android:id="@+id/Heading"
                android:textSize="15sp"
                android:textColor="@color/Text"
                android:gravity="center"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="@string/ScheduleOfCalls_no_n"
                android:fontFamily="@font/golos_text_medium"/>

        </LinearLayout>

        <LinearLayout
            android:gravity="end"
            android:orientation="horizontal"
            android:layout_width="95dp"
            android:layout_height="match_parent">

            <Button
                android:theme="@style/Button.Menu"
                android:layout_marginEnd="6dp"
                android:gravity="end"
                android:id="@+id/Menu"
                android:layout_width="62dp"
                android:layout_height="match_parent"
                android:onClick="Menu"
                android:paddingHorizontal="20dp"
                app:icon="@drawable/ic_button_menu"
                app:iconTint="@color/BrandColor"
                style="@style/Widget.MaterialComponents.Button.TextButton"/>

        </LinearLayout>

    </TableRow>

